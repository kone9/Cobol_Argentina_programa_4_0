    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="VALORhora pic 9(9999)"/>
            <declare name="VALORhora" type="Integer" array="False" size=""/>
            <comment text="CANThorasdiarias pic 9(9999)"/>
            <declare name="CANThorasdiarias" type="Integer" array="False" size=""/>
            <comment text="VALORsueldosem pic 9(9999)"/>
