            <output expression="&quot;INGRESE EL VALOR DE LA HORA TRABAJADA&quot;" newline="True"/>
            <input variable="VALORhora"/>
            <comment text="INGRESE SUS CANTIDAD DE HORAS DIARIAS TRABAJADAS EN D&#205;A H&#193;BIL"/>
